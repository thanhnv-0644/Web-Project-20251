spring.application.name=Phegon-Eccormerce
server.port=2424

# Database Configuration
spring.datasource.url=jdbc:mysql://localhost:3307/phegon_ecommerce
spring.datasource.username=root
spring.datasource.password=YOUR_DATABASE_PASSWORD_HERE

spring.jpa.hibernate.ddl-auto=update
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

# JWT Secret Key
# Generate your own secure key: https://www.allkeysgenerator.com/Random/Security-Encryption-Key-Generator.aspx
secreteJwtString=YOUR_SECRET_JWT_KEY_HERE

# Cloudinary Configuration (FREE - Không cần thẻ tín dụng)
# Đăng ký tại: https://cloudinary.com/users/register/free
# Sau khi đăng ký, lấy credentials từ Dashboard
cloudinary.cloud-name=your_cloud_name_here
cloudinary.api-key=your_api_key_here
cloudinary.api-secret=your_api_secret_here

